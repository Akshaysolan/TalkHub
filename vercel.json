{
  "version": 2,
  "builds": [
    {
      "src": "chat_app/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chat_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "chat_project_name.settings",
    "SECRET_KEY": "django-insecure-!@#$%^&*()_+",
    "DEBUG": "True",
    "ALLOWED_HOSTS": "*"
  }
}